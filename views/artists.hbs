<a href="/dashboard/artist/create">create Artist</a>
<table>
    <tbody>
        <tr>
        <th>Name</th>
        <th>Band?</th>
        <th>Description</th>
        <th>Picture</th>
        </tr>
{{#each artists}}
    <tr>
        <td>{{this.name}}</td>
        <td>
            {{#if this.isBand}}
                "Its a band"
            {{/if }}
        </td>
        <td>{{this.description}}</td>
        <td><img src="{{this.picture}}" alt="{{this.name}}" width="150px"></td>
        <td>
            <a href="/dashboard/artist/delete/{{this._id}}">
                Delete Artist
            </a>
        </td>
        <td>
             <a href="/dashboard/artist/update/{{this._id}}">
                Update Artist
            </a>
        </td>
    </tr>
{{/each}}
</tbody>
</table>